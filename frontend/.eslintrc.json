{
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "no-console": "error",
        "no-debugger": "error"
    },
    "env": {
        "browser": true,
        "node": true
    }
}